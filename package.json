{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "open:dev": "opener http://localhost:3000",
    "test": "NODE_ENV=test JAMRUN=test mocha test --recursive",
    "setup": "node sessionSetup.js",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "start": "gulp",
    "build": "gulp build",
    "startForever": "forever start --spinSleepTime 10000 server/server.js",
    "deploy": "npm run build && npm run startForever",
    "restartForever": "forever restart server/server.js && forever restart compression_server/bin/www",
    "killnode": "killall -9 node",
    "startPSQL": "pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start",
    "stopPSQL": "pg_ctl -D /usr/local/var/postgres stop -s -m fast"
  },
